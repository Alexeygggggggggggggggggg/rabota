#include <iostream>
#include <ctime>
using namespace std;
int main()
{
setlocale(LC_ALL, "rus");
	int sum = 0;
	const int n = 8;
	int arr[n];
	srand(time(0));
	for (int i = 0; i < n; i++) {
		arr[i] = rand() % 21 - 10;
		if (arr[i] > 0) {
			sum += arr[i];
		}
	}
	for (int i = 0; i < n; i++) { cout << ' ' << endl; }
		cout << sum << endl;

		return 0;
}
